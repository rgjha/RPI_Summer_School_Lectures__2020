        "import numpy as np  \n",
        "from ncon import ncon\n",
        "\n",
        "B = np.random.rand(3,3,3,3)\n",
        "C = np.random.rand(3,3,3,3)\n",
        "D = np.random.rand(3,3,3,3)\n",
        "A = ncon((B, C, D),([1,2,3,4],[2,1,-2,-1],[4,3,-4,-3]))\n",
        "A1 = np.einsum('ijkl,jiqr,lkab->rqba',B,C,D) \n",
        "print (np.allclose(A,A1))"
      ],
